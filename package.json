{
  "name": "reversi-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start && prettier-watch",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier-watch": "onchange '**/*.js' '**/*.ts' -e 'dist/**/**' 'node_modules/**/*' -- prettier --write {{changed}}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "node-sass": "^4.13.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "^3.3.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.21",
    "@types/node": "12.12.5",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.3",
    "prettier": "1.18.2",
    "typescript": "3.6.4"
  }
}
