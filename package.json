{
  "name": "reversi-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start && prettier-watch",
    "prettier-watch": "onchange '**/*.js' '**/*.ts' -e 'dist/**/**' 'node_modules/**/*' -- prettier --write {{changed}}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "autoprefixer": "^10.0.0",
    "easytimer.js": "^4.1.1",
    "next": "^11.1.0",
    "postcss": "^7.0.32",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "^3.3.0",
    "sass": "^1.26.11",
    "tailwindcss": "^1.8.10",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@types/jest": "24.0.21",
    "@types/node": "12.12.5",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "prettier": "^1.18.2"
  }
}
